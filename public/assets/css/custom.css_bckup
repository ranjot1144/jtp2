        padding: 8px 8px 8px 0px;
        text-decoration: none;
        font-size: 14px;
        color: #818181;
        display: block;
        transition: 0.3s;
    }

    .site-logo {
        padding: 0px;
    }

    .zeynep .site-logo {
        padding: 15px !important;
        margin-bottom: 0px;

        a {
            padding: 0px;
        }
    }

    .zeynep.opened .sidebar-bottom {
        .social_icons {
            margin: 10px 0px !important;

            a {
                padding: 0px;
            }
        }

        p a {
            padding: 0px;
        }
    }

    header .sidenav a img {
        width: 65%;
    }

    header .sidenav a:hover {
        color: #f1f1f1;
    }

    header .contact_btn {
        position: absolute;
        bottom: 0;
        width: 88%;
        margin: 32px 15px;
    }

    header .contact_btn .btn {
        width: 100%;
        background-color: #ad03bf;
        justify-content: center;
        color: #fff;
    }

    .flexdropdownmenu {
        display: none !important;
        visibility: hidden !important;
    }

    .intro_section {
        overflow: hidden !important;
    }

    .sidenav.menu-expand {
        width: 80%;
    }

    .search-container {
        max-width: unset;
    }

    .main-content .container-fluid {
        padding-left: 0;
        padding-right: 0;
    }

    .overlay-menu {
        background: rgba(0, 0, 0, 0.5);
        position: fixed;
        width: 100%;
        height: 100vh;
        left: 0;
        top: 0;
        z-index: 3;
    }

    img.image-50 {
        height: 7pc;
        margin-bottom: 20px;
        object-fit: contain;
    }

    figure img {
        width: -webkit-fill-available;
        height: auto;
    }

    .site-section {
        padding: 5em 0em;
    }

    .breadcrumbs {
        margin-top: 98px;
    }
}

@media (max-width: 599px) {
    #industry-section-carousel .owl-stage .owl-item img {
        width: 120px !important;
        height: 140px !important;
    }

    .col-md-3.ranges_prod {
        width: 50%;
    }
}

@media (max-width: 500px) {
    .col-md-3.ranges_prod {
        width: 100%;
    }

    .site-section {
        padding: 15px !important;
    }
}


@media (max-width: 400px) {
    #industry-section-carousel .owl-stage .owl-item img {
        width: 100px !important;
        height: 120px !important;
    }

    .top-grade-bar>div {
        font-size: 10px;
    }

    .mobile-Grades-container ul li {
        font-size: 12px;
    }

    .header-grade {
        padding: 11px 2px;
        gap: 10px;
    }
}