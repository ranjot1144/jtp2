.overflow-x-hidden {
    overflow-x: hidden;
}

html {
    scroll-behavior: smooth;
}

.floating {
    animation-name: floating;
    animation-duration: 3s;
    animation-iteration-count: infinite;
    animation-timing-function: ease-in-out;
    margin-left: 0px;
}

#grades_table table tbody td:first-child {
    background-color: #F47922;
    border-radius: 30px 0px 0px 30px;
    font-weight: 600;
    color: #fff;
}

.child-row1 td:first-child {
    background-color: #fff !important;
    border-radius: 0 !important;
}

@keyframes floating {
    from {
        transform: translate(0, 0px);
    }

    65% {
        transform: translate(0, 15px);
    }

    to {
        transform: translate(0, 0px);
    }
}

.bg-image-header {
    background-position: 10% 192.1px !important;
}

.arrow_icon {
    margin-top: 30px;
}

.arrow_icon img {
    max-width: 50px;
}

header .mobile_logo {
    display: none;
}

header .megamenu_d {
    background-size: cover;
    width: 295px;
    bottom: 0;
    overflow: hidden;
    -webkit-overflow-scrolling: touch;
    overflow-y: auto;
    position: fixed;
    top: 0;
    z-index: 9;
}

header .megamenu_d .site-logo {
    /* background: #fff; */
    padding: 15px;
    margin-bottom: 0px;
}

header .megamenu_d .flexdropdownmenu {
    display: none;
}

header .sidebar-bottom {
    color: #fff;
    position: absolute;
    bottom: 10px;
    z-index: 99999;

    width: 100%;
}

header .sidebar-bottom .sidebar_rel {
    padding: 10px 45px;
    position: relative;
}

header .sidebar-bottom .sidebar_rel .privacy_sec {
    display: grid;
    font-size: 14px;
}

header .sidebar-bottom .images-content {
    display: flex;
    gap: 20px;
    padding: 15px 0px 26px;
}

header .sidebar-bottom a img {
    width: 90px;
}


header .sidebar-bottom .sidebar_rel .privacy_sec a {
    color: #fff;
}

header .sidebar-bottom .sidebar_rel .sidebar-bottom-img {
    display: flex;
    gap: 0px 15px;
    width: 100%;
    margin-top: 30px;
}

header .sidebar-bottom p {
    color: #818181;
}

header .sidebar-bottom .sidebar_rel .sidebar-bottom-img img {
    width: 100%;
}

header .sidebar-bottom .sidebar_rel .social_icons {
    margin-top: 30px;
    display: flex;
    justify-content: space-between;
    padding-bottom: 18px;
}

header .sidebar-bottom .sidebar_rel .social_icons a {
    color: #818181;
}

.search-container {
    max-width: 400px;
    position: relative;
    margin: auto;
}

.mobilemenu {
    display: none;
}

.mobileSidebarContainer {
    display: none;
}

#industry-section-carousel .owl-carousel .owl-stage {
    display: flex;
    align-items: center;
    padding-top: 36px;
    padding-bottom: 36px;
}

#industry-section-carousel .owl-carousel .owl-stage p {
    margin: 0;
}

#industry-section-carousel .owl-stage .owl-item img {
    padding-top: 0;
}


.owl-carousel.slide-one-item.home-slider.owl-loaded.owl-drag {
    padding: 0;
}

.cellulose-table-box tr td {
    border-radius: 10px;
}

.cellulose-table-box tbody {
    border-left: 0;
    border-right: 0;
}

.cellulose-table-box tbody tr:last-child td:first-child {
    border-radius: 10px 10px 28px 28px;
}


.cellulose-table-box .data-box {
    background-color: red;
}

.cellulose-table-box .data-box td {
    border-radius: 0 !important;
}

.mobile-Grades-container {
    width: 100%;
}

.top-grade-bar {
    display: flex;
    justify-content: space-between;
    background-image: linear-gradient(to right, #d3552e, 35%, #F58621);
    border-radius: 20px 20px 0 0px;
    padding: 11px 23px;
    margin-bottom: 7px;
}

.grade-content-area {
    display: flex;
    flex-direction: column;
    gap: 7px;
}

.top-grade-bar>div {
    font-size: 15px;
    font-weight: 600;
    color: #fff;
}

.grade-box {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.mobile-Grades-container {
    display: none;
}

.mobile-Grades-container ul {
    padding: 0;
    list-style: none;
    padding-top: 7px;
}

.mobile-Grades-container ul li {
    display: flex;
    justify-content: space-between;
    margin-bottom: 10px;
    font-size: 15px;
    font-weight: 400;
    align-items: center;
}

.grade-items {
    padding-right: 20px;
}

.mobile-Grades-container .grade-num {
    background-color: #F47922;
    border-radius: 30px 0px 0px 30px;
    font-weight: 600;
    color: #fff;
    align-items: center;
    gap: 15px;
    display: flex;
    padding: 4px 18px 4px 23px;
    font-size: 17px;
}

.mobile-Grades-container .arrow {
    border: solid #fff;
    border-width: 0 3px 3px 0;
    display: inline-block;
    padding: 3px;
    margin-right: 10px;
    margin-bottom: 6px;
    transform: rotate(45deg);
}

.mobile-Grades-container .dropdonw-content {
    max-height: 0;
    transition: 2s;
    overflow: hidden;
}

.mobile-Grades-container .dropdonw-content.show {
    max-height: 2000px;
}

.header-grade {
    display: flex;
    background: #ebecec;
    padding: 13px 23px;
    justify-content: space-between;
}

.header-grade>div {
    color: #000;
    font-weight: 700;
    text-align: center;
    font-size: 11px;
}

.header-grade>div:first-child {
    width: 50%;
}

.mobile-Grades-container .grade-points {
    display: flex;
    padding: 11px 0;
}

.mobile-Grades-container .grade-points h6 {
    width: 50%;
    font-size: 10px;
    font-weight: 400;
    margin: 0;
}

.mobile-Grades-container .grade-points ul {
    width: 50%;
}

.mobile-Grades-container .grade-points ul li {
    justify-content: space-around;
}

@media (max-width: 1024px) {
    .owl-stage .owl-item .item img {
        height: 174px !important;
    }
}

@media only screen and (max-width: 768px) {
    .col-md-3.ranges_prod {
        width: 33.3%;
    }

    .bg-image-header {
        background-position: 10% 679.1px !important;
    }

    .bg-image {
        background-attachment: initial !important;
    }
